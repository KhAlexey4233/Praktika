            <DataGrid x:Name="SupplyDataGrid" Grid.RowSpan="2" Grid.ColumnSpan="2" Margin="10" IsReadOnly="True" AutoGenerateColumns="False" SelectionChanged="SupplyDataGrid_SelectionChanged" ColumnWidth="Auto">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Цена" Binding="{Binding Price}" />
                <DataGridTextColumn Header="Агент" Binding="{Binding Agent}" />
                <DataGridTextColumn Header="Клиент" Binding="{Binding Client}" />
                <DataGridTextColumn Header="Объект недвижимости" Binding="{Binding RealEstate}" />
            </DataGrid.Columns>
        </DataGrid>
        </StackPanel>

        <StackPanel Grid.Row="1" Grid.Column="2" Margin="10">

            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />

                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>

                <Label Content="Объект" Grid.Column="0" Grid.Row="0" />
                <ComboBox Style="{DynamicResource ComboBoxStyle1}" x:Name="RealEstateCb" Grid.Column="0" Grid.Row="1" SelectionChanged="RealEstateCb_SelectionChanged"/>

                <Label Content="Клиент" Grid.Column="0" Grid.Row="2" />
                <ComboBox Style="{DynamicResource ComboBoxStyle1}" x:Name="ClientCb" Grid.Column="0" Grid.Row="3" SelectionChanged="ClientCb_SelectionChanged" />

                <Label Content="Агент" Grid.Column="1" Grid.Row="0" />
                <ComboBox Style="{DynamicResource ComboBoxStyle1}" x:Name="AgentCb" Grid.Column="1" Grid.Row="1"/>

                <Label Content="Цена" Grid.Column="1" Grid.Row="2" />
                <TextBox x:Name="PriceTextBox" Grid.Column="1" Grid.Row="3"/>
            </Grid>
            <Button x:Name="AddWindow" Content="Добавить" Margin="0,20,0,5" Click="AddWindow_Click" />
            <Button x:Name="Update" Content="Обновить" Margin="0,10,0,5" Click="Update_Click" />
            <Button x:Name="Delete" Content="Удалить" Margin="0,10" Click="Delete_Click" />
            <Button x:Name="Back" Content="Отменить" Margin="0,5" Click="Back_Click" Visibility="Hidden" />
        </StackPanel>
        <StackPanel Grid.Row="1" Grid.Column="2" Margin="10" />
    </StackPanel>
</Window>
